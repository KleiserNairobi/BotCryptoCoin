generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Config {
  id        String    @id @default(uuid())
  email     String    @unique
  password  String
  apiUrl    String
  accessKey String
  secretKey String
  createdAt DateTime  @default(now()) @map("created_at")
  updatedAt DateTime? @updatedAt @map("updated_at")

  @@map("configuracoes")
}
